# Intent CLI Environment Variables
# Copy this file to .env and fill in your values
# .env files are automatically loaded if present in the project root

# API Configuration
# Local development: http://localhost:8080
# Production: https://api.intentregistry.com
INTENT_API_URL=http://localhost:8080

# Authentication Token
# Get your token from the IntentRegistry API
# For local dev, you may need to create a token in your local API
# Leave empty if your local API doesn't require authentication
INTENT_TOKEN=

# Telemetry (optional)
# Set to "true" or "1" to enable telemetry
INTENT_TELEMETRY=false

# Package Signing Key (optional)
# Path to ed25519 private key for signing packages
# Default: ~/.ssh/intent_sign_key.hex
INTENT_SIGN_KEY=
